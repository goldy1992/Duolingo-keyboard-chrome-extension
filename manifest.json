{
  "name": "Mike's Ukranian Extension",
  "manifest_version": 2,
	"version": "1.0",
  "description": "Converts Keyboard into Ukranian for Ukranian questions",
  "icons": { "128": "ukr128.png" },
 

	
  "permissions": [
		"activeTab",
		"https://www.duolingo.com/skill/*", 
		"https://www.duolingo.com/practice*"],
		
  "author": "Michael Goldsmith",
		
  "page_action": {
    "default_title": "Ukranian Duolingo Extension",
    "default_icon": {"19": "ukr19.png", 
            "38": "ukr38.png"}, 
		  "background": {
				"persistent": false
				}
  },
	
	  "content_scripts": [
    {
      "matches": [		"https://www.duolingo.com/skill/*", 
											"https://www.duolingo.com/practice*"],
      "js": ["jquery-2.1.4.min.js", "jquery.caret.js", "keyMap.js", "runScript.js"]
    }
  ]
}